												 srcset="{{ currency.iso_code  | slice: 0, 2 | handle | append: '.' | append: file_extensionn | asset_url }}"
												 class="country--flag {{ currency.iso_code }}"
												 alt="{{ currency.iso_code }}"> {{ currency.iso_code }}
										</picture>
									</a>
								</li>
							{% endfor %}
						</ul>
					</li>
				{% endif %}
			{% endif %}

			{% if settings.enabletopnavonmobile %}
				{% for link in linklists[settings.strip_linklist].links %}
					<li {% if link.active %}class="site-nav--active"{% endif %}>
						<a class="site-nav__linkk" href="{{ link.url }}">{{ link.title }}</a>
					</li>
				{% endfor %}
			{% endif %}

			{% if shop.customer_accounts_enabled %}
				<li>
					<a href="/account" class="site-nav__linkk">{{ 'customer.account.title' | t }}</a>
				</li>

				{% if customer %}
					<li>
						<a href="/account/logout">{{ 'customer.login.logout' | t }}</a>
					</li>
				{% endif %}
			{% endif %}

			{% if settings.enable_header_contact %}
				<li class="center menu-contact">
					<p class="hsit">
						{{ settings.header_question_line }}
					</p>

					<h5>
						<a href="tel:{{ settings.header_phone }}">
							{% if settings.usecustomphoneicon == true %}
								<picture class="lazy">
									<img data-srcset="{{ 'phone-icon.png' | asset_img_url: '30x' }} 1x, {{ 'phone-icon.png' | asset_img_url: '60x' }} 2x"
										 srcset="{{ 'phone-icon.png' | asset_img_url: '5x' }}"
										 class="picon"
										 alt="Phone Icon">
								</picture>
							{% else %}
								<span class="fa fa-phone"></span>
							{% endif %}

							{{ settings.header_phone }}
						</a>
					</h5>

					<p>
						<strong>{{ settings.header_time }}</strong>
					</p>
				</li>
			{% endif %}
			{% if settings.deal_of_the_day_enabled and settings.deal_of_the_day_start_date != blank %}
				<li class="deal-of-the-day-menu-item">
					{% include 'deal-of-the-day' %}
				</li>
			{% endif %}
		</ul>
	</nav>
</div>


