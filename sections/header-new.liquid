<div class="menu-bg js-menu-overlay"></div>
<div class="search-overlay"></div>

<header class="header js-header container-fluid{% if settings.enable_sticky_headerr == "three" %} perma-sticky-mobile{% endif %}{% if settings.enable_sticky_headerr == "two" or settings.enable_sticky_headerr == "one" %} perma-sticky js-header-mini-sticky{% endif %}">
	{% if settings.enable_topcbar %}
		{% if settings.coupan != '' %}
			<div class="row top_bar_save no-gutters js-top-bar-save">
				<div class="container">
					<p class='coupon-text'>
						Coupon Code: <span id="coupan_code_copy">{{ settings.coupan }}</span>
						<span class="button_copy_code">Copy Code</span>
					</p>
				</div>

				<div class="close-btn js-top-bar-save-close">
					<i class="fas fa-times"></i>
				</div>
			</div>
		{% endif %}
	{% endif %}

	<div class="row top_bar no-gutters {% if settings.enable_sticky_headerr == "two" %}no-border-sticky js-hide-when-scroll{% endif %}">
		<div class="container header-strip">
			{% if settings.enable_header_fb_page and settings.header_fb_page_url != blank %}
				<div class="fblikebar">
					<div class="cbha">
						<div id="fb-root"></div>
						<div class="fb-like"
							 data-href="{{ settings.header_fb_page_url }}"
							 data-layout="button_count"
							 data-action="like"
							 data-size="small"
							 data-show-faces="false"
							 data-share="true"></div>
					</div>
				</div>
			{% endif %}

			{% if settings.enable_strip_text %}
				{% assign product_motivator_text = settings.strip_text %}
				{% assign strip_display_user_country = settings.strip_display_user_country %}
				{% assign free_shipping_text = settings.free_shipping_text_free_cart_page %}

				<div class="shipping_bar js-product-motivator-text"
					 data-motivator-text="{{ product_motivator_text }}"
					 data-free-shipping-text="{{ free_shipping_text }}"
				>
					{% if settings.strip_url != blank %}
						<a style="color: inherit;"
						   class="free_shipping_top_strip"
						   href="{{ settings.strip_url }}"
						>
							<span class="js-product-motivator-text-main"></span>
						</a>
					{% else %}
						<span class="js-product-motivator-text-main"></span>
					{% endif %}

					{% if strip_display_user_country %}
						<span class="js-country-name js-product-motivator-text-country product-motivator-text__country"></span>
						<span class="js-country-flag js-product-motivator-text-flag product-motivator-text__flag"></span>
					{% endif %}
				</div>
			{% endif %}

			<nav class="main-nav topnav">
				<ul class="tpn">
					{% if settings.show_multiple_currencies %}
						<li class="dropdown">
							{% include 'currencies-switcher-new' %}
						</li>
					{% endif %}

					{% for link in linklists[settings.strip_linklist].links %}
						<li>
							<a href="{{ link.url }}">{{ link.title }}</a>
						</li>
					{% endfor %}

					{% if shop.customer_accounts_enabled %}
						<li>
							<a href="/account">{{ 'customer.account.title' | t }}</a>
						</li>

						{% if customer %}
							<li>
								<a href="/account/logout">{{ 'layout.customer.log_out' | t }}</a>
							</li>
						{% endif %}
					{% endif %}
				</ul>

				<div class="cartatt">
					<ul class="cart nm">
						{% if template !="cart" %}
							<li class="top-nav-link cart__hover">
								<a href="/cart"
								   class="top-nav-link site-header__cart-toggle js-drawer-open-right"
								   aria-controls="CartDrawer" aria-expanded="false"
								>
									{% if settings.usecutomiconforcart %}
										<picture class="lazy">
											<img data-srcset="{{ 'cart.png' | asset_img_url: '30x' }} 1x, {{ 'cart.png' | asset_img_url: '60x' }} 2x"
												 srcset="{{ 'cart.png' | asset_img_url: '5x' }}"
												 class="cartimga nonalt"
												 alt="Cart Icon">
										</picture>

										<picture class="lazy">
											<img data-srcset="{{ 'cart-alt.png' | asset_img_url: '30x' }} 1x, {{ 'cart-alt.png' | asset_img_url: '60x' }} 2x"
												 srcset="{{ 'cart-alt.png' | asset_img_url: '5x' }}"
												 class="cartimga altt"
												 alt="Cart Icon">
										</picture>
									{% else %}
										<i class="fa fa-shopping-cart"></i>
									{% endif %}

									<span class="CartCount{% if cart.item_count < 1 %} hidden{% endif %}">{{ cart.item_count }}</span>
								</a>

								<div class="js-static-sticky-cart-wrapper">
									{% if cart.item_count > 0 %}
                                      <div id="cart-popup">
                                        <form action="/cart" method="post" novalidate class="cartt ajaxcart">
                                          <div class="container">
                                            <div class="cartForm">
                                              <ul class="cart-popup-ul-middle">
                                                {% for item in cart.items %}
                                                <li>
                                                  <a href="{{ item.url }}">
                                                    <div class="cart-img-div">
                                                      <img src="{{ item | img_url: 'small' }}" alt="Image of {{ item.title | escape }}" class="cart-item-image" />
                                                      <span class="pop-qty-crat">{{ item.quantity }}</span>
                                                    </div>
                                                    <div class="cart-item-info">
                                                      <span class="cart-item-title">{{ item.title }}</span>
                                                      <p class="cart-item-price"><span class="money">{{ item.line_price | money }}</span></p>
                                                    </div>
                                                  </a>
                                                  <a href="/cart/change?line={{ forloop.index }}&quantity=0" data-line="{{ forloop.index }}" data-variant="{{ item.variant.id }}" class="removeLineCartPop" rel="{{ item.variant.id }}"><i class="fa fa-times"></i></a>
                                                </li>
                                                {% endfor %}
                                              </ul>
                                              <ul class="cart-popup-ul-top">
                                                <li class="total-items">
                                                  <b>{{ 'cart.general.total' | t }}:  <span class="money">{{ cart.total_price | money }}</span></b>
                                                </li>
                                              </ul>
                                              <ul class="cart-popup-ul-bottom">
                                                <li>
                                                  <a href="/cart" class="btn--secondary btn--full cart__cartview">{{ "cart.general.button" | t }}</a>
                                                </li>
                                                <li>
                                                  <a href="/checkout" class="btn btn--full cart__checkout Checkout">
                                                    <img src="{{ 'checkout-button-icon.png' | asset_url }}" alt="" class="addIcon" />
                                                    <span id="CheckoutText">{{ 'cart.general.checkout' | t }}</span>
                                                  </a>
                                                </li>
                                                <li class="additional-checkout-buttons">
                                                  <button name="goto_pp" type="submit" id="paypal-express-button" class="additional-checkout-button additional-checkout-button--paypal-express" value="paypal_express" data-strategy="cart">Check out with <img alt="Checkout with: PayPal" src="//cdn.shopify.com/s/assets/checkout/easy-checkout-btn-paypal-9835af2c2b0e2a543b2905789a7f08b678d62de2c77c1b0d16fd7689aff463f3.png"></button>
                                                </li>
                                                <li>
                                                  <img alt="Checkout Secure" src="{{ 'checkout_icon.png' | asset_url }}" class="no-border checkout-img ratina-img">
                                                  <div class="cart--promo-msg common_shipping_text"></div>
                                                </li>
                                              </ul>
                                            </div>
                                          </div>
                                        </form>
                                      </div>
									{% else %}
                                      <div id="cart-popup" class="empty-popup">
                                        <div class="container">
                                          <ul class="cart-popup-ul-bottom">
                                            <li class="empty-cart-popup-msg">{{ 'cart.general.empty' | t }}</li>
                                            <li>
                                              <a href="/collections/all" class="btn btn-large btn-primary">{{ 'cart.general.shop_now' | t }}</a>
                                            </li>
                                          </ul>
                                        </div>
                                      </div>
									{% endif %}
								</div>
							</li>
						{% endif %}
					</ul>
				</div>
			</nav>
		</div>
	</div>

	<div class="row middle_bar no-gutters">
		{% if settings.enable_mobile_topbar %}
			<div class="top-line-wrapper {% if settings.enable_sticky_headerr == "two" %} js-hide-when-scroll{% endif %}">
				<div class="top--line mshop{% if settings.mobile_top_strip contains '$x' %} js-product-motivator-text{% endif %}" data-motivator-text="{{- settings.mobile_top_strip -}}">
				<span class="mobile_bar_shipping_text{% if settings.mobile_top_strip contains '$x' %} js-product-motivator-text-main{% endif %}">
					{{- settings.mobile_top_strip -}}
				</span>

					{% if settings.enable_banner_user_country %}
						<span class="js-country-name"></span>&nbsp;
						<span class="js-country-flag"></span>
					{% endif %}
				</div>
			</div>
		{% endif %}

		<div class="container" {% if template =="cart" %}style="height: 50px;"{% endif %}>
			<div class="menu-btn {% if template =="cartt" %}medium-down--hide large--hide{% endif %}">
				<a href="#menu" class="js-mobile-menu-toggle">
					<span class="fa fa-bars"></span>
				</a>
			</div>

			{% if settings.enable_header_search %}
				<div class="search js-search-bar-button{% if template =="cartsearcht" %} medium-down--hide large--hide{% endif %}{% if settings.mobile_search_options == 'two' %} js-show-mobile-when-scroll{% endif %}{% if settings.mobile_search_options == 'three' %} js-search-bar-button-focus{% endif %}">
					{% if settings.usesearchicon %}
						<img src="{{ 'searchicon.png' | asset_url }}" class="search_icon">
					{% else %}
						<span class="fa fa-search"></span>
					{% endif %}
				</div>
			{% endif %}

			<div class="noproclas col-md-3">
				<div class="row">
					<div class="logo">
						{% if settings.logo_use_image %}
							<a href="/" class="desktop-logo">
								{% if settings.logo_png != blank %}
									<img src="{{ settings.logo_png | img_url: 'master' }}"
										 class="img-responsive"
										 height="70"
										{% if settings.logo_max_width != '' %} style="width:{{ settings.logo_max_width }}px" {% endif %}
										 alt="{{ shop.name }}">
								{% else %}
									<img src="{{ 'logo.png' | asset_img_url: 'master' }}"
										 class="img-responsive"
										 height="70"
										{% if settings.logo_max_width != '' %} style="width:{{ settings.logo_max_width }}px" {% endif %}
										 alt="{{ shop.name }}">
								{% endif %}
							</a>
						{% else %}
							<a href="/">{{ shop.name }}</a>
						{% endif %}

						<a href="/" class="mobile-logo">
							{% if settings.logo_png != blank %}
								<img src="{{ settings.logo_png | img_url: '145x' }}" alt="{{ shop.name }}" class="img-responsive" height="28" width="145">
							{% else %}
								<img src="{{ 'logo.png' | asset_url }}" alt="{{ shop.name }}" class="img-responsive" height="28" width="145">
							{% endif %}
						</a>
					</div>
				</div>
			</div>

			<div class="cart menu-btn">
				<a href="/cart"
				   class="top-nav-link site-header__cart-toggle js-drawer-open-right"
				   aria-controls="CartDrawer" aria-expanded="false" style="margin-top: 0;">
					{% if settings.usecutomiconforcart == true %}
						<picture class="lazy">
							<img data-srcset="{{ 'cart-alt.png' | asset_img_url: '30x' }} 1x, {{ 'cart-alt.png' | asset_img_url: '60x' }} 2x"
								 srcset="{{ 'cart-alt.png' | asset_img_url: '5x' }}"
								 class="cartimga"
								 alt="Cart Icon">
						</picture>
					{% else %}
						<i class="fa fa-shopping-cart{% unless  settings.enable_sticky_headerr == 'one'%} fa-shopping-cart--sticky-color{% endunless %}"></i>
					{% endif %}
					<span class="CartCount{% if cart.item_count < 1 %} hidden{% endif %}">{{ cart.item_count }}</span>

				</a>
			</div>

			<div class="{% if settings.enable_header_promotion %}col-lg-4 {% else %}col-lg-6 {% endif %}tabl">
				{% if settings.enable_header_search %}
					<div class="search_panel{% if settings.mobile_search_options == 'two' %} js-hide-mobile-when-scroll{% endif %}{% if settings.mobile_search_options == 'one' %} hidden{% endif %} js-search-bar-wrapper{% if settings.usesearchicon %} search_panel--padding{% endif %}">
						{% include 'search-bar' %}
					</div>
				{% endif %}
			</div>

			{% if settings.enable_header_promotion %}
				<div class="col-md-2 mob-hide no-padding">
					{% include 'deal-of-the-day' %}

					{% unless settings.deal_of_the_day_enabled %}
						<div class="header-promotion-image">
							{% if settings.header_promotion_link != '' %}
								<a href="{{ settings.header_promotion_link }}">
									<picture class="lazy">
										<img data-srcset="{{ 'header_image.png' | asset_img_url: '220x' }} 1x, {{ 'header_image.png' | asset_img_url: '440x' }} 2x"
											 srcset="{{ 'header_image.png' | asset_img_url: '20x' }}"
											 alt="Image of {{ shop.name | escape }}">
									</picture>
								</a>
							{% else %}
								<picture class="lazy">
									<img data-srcset="{{ 'header_image.png' | asset_img_url: '220x' }} 1x, {{ 'header_image.png' | asset_img_url: '440x' }} 2x"
										 srcset="{{ 'header_image.png' | asset_img_url: '20x' }}"
										 alt="Image of {{ shop.name | escape }}">
								</picture>
							{% endif %}
						</div>
					{% endunless %}
				</div>
			{% endif %}

			{% if settings.enable_header_contact %}
				<div class="col-md-3 mob-hide header__col-contact{% if settings.optionofphone == 'Standard'%} contact--show{% endif %}">
					{% unless settings.optionofphone == 'Menu' %}
						<div class="contact">
							<p class="hsit">{{ settings.header_question_line }}</p>

							{% capture content_for_phone_link %}
								{% if settings.usecustomphoneicon %}
									<picture class="lazy">
										<img data-srcset="{{ 'phone-icon.png' | asset_img_url: '20x' }} 1x, {{ 'phone-icon.png' | asset_img_url: '40x' }} 2x"
											 srcset="{{ 'phone-icon.png' | asset_img_url: '5x' }}"
											 class="picon"
											 alt="Phone Icon">
									</picture>
								{% else %}
									<span class="fa fa-phone"></span>
								{% endif %}

								{{ settings.header_phone }}
							{% endcapture %}

							<h5>
								{% if settings.linkurlforphone %}
									<a href="{{ settings.urlforphone }}">
										{{- content_for_phone_link -}}
									</a>
								{% else %}
									<a href="tel:{{ settings.header_phone }}">
										{{- content_for_phone_link -}}
									</a>
								{% endif %}
							</h5>

							<p class="boe">
								<strong>{{ settings.header_time }}</strong>
							</p>
						</div>
					{% endunless %}

					{% if settings.enable_sticky_headerr == "two" %}
						<div class="cartattnew js-show-when-scroll hidden">
							<ul class="cart">
								<li class="top-nav-link cart__hover">
									<a data-no-instant="" href="/cart"
									   class="top-nav-link site-header__cart-toggle js-drawer-open-right"
									   aria-controls="CartDrawer" aria-expanded="false" style="margin-top: 0;">
										{% if settings.usecutomiconforcart == true %}
											<picture class="lazy">
												<img data-srcset="{{ 'cart-alt.png' | asset_img_url: '30x' }} 1x, {{ 'cart-alt.png' | asset_img_url: '60x' }} 2x"
													 srcset="{{ 'cart-alt.png' | asset_img_url: '5x' }}"
													 class="cartimga altt"
													 alt="Cart Icon">
											</picture>
										{% else %}
											<i class="fa fa-shopping-cart{% unless  settings.enable_sticky_headerr == 'one'%} fa-shopping-cart--sticky-color{% endunless %}"></i>
										{% endif %}
										<span class="CartCount{% if cart.item_count < 1 %} hidden{% endif %}">{{ cart.item_count }}</span>
									</a>

									<div class="js-scroll-sticky-cart-wrapper"></div>
								</li>
							</ul>
						</div>
					{% endif %}
				</div>
			{% endif %}
		</div>
	</div>

	<div class="row menu_bar no-gutters">
		<div class="container">
			<div class="menu{% if settings.showhighlightbar %} menu--show-highlight-bar{% endif %}{% if settings.alignment_of_nav == 'center' %} menu--center-mode{% endif %}">
				<ul>
					{% for link in linklists[section.settings.header_linklist_1].links %}
						{% assign child_list_handle = link.title | handle %}

						{% if linklists[child_list_handle] and linklists[child_list_handle].links.size > 0 %}
							<li class="site-nav--has-dropdown menu-dropdown-icon js-dropdown-menu-link{% if link.active %} site-nav--active{% endif %}"
								aria-haspopup="true">

								<a href="{{ link.url }}" class="menu-open">
									{{ link.title }}
									<span class="fa fa-angle-down"></span>
								</a>

								<ul class="site-nav__dropdown normal-sub slides">
									{% for child_link in linklists[child_list_handle].links %}
										{% assign grand_child_list_handle = child_link.title | handle %}

										{% if linklists[grand_child_list_handle] and linklists[grand_child_list_handle].links.size > 0 %}
											<li class="site-nav--has-dropdown menu-dropdown-icon">
												<a href="{{ child_link.url }}" class="site-nav__linkk">
													{{ child_link.title }}
													<span class="fa fa-angle-right"></span>
												</a>

												<ul class="site-nav__dropdown normal-sub-sub slides">
													{% for grand_child_link in linklists[grand_child_list_handle].links %}
														<li>
															<a href="{{ grand_child_link.url }}"
															   class="site-nav__linkk">
																{{ grand_child_link.title }}
															</a>
														</li>
													{% endfor %}
												</ul>
											</li>
										{% else %}
											<li>
												<a href="{{ child_link.url }}" class="site-nav__linkk">
													{{ child_link.title }}
												</a>
											</li>
										{% endif %}
									{% endfor %}
								</ul>
							</li>
						{% else %}
							<li {% if link.active %}class="site-nav--active"{% endif %}>
								<a href="{{ link.url }}" class="site-nav__linkk">{{ link.title }}</a>
							</li>
						{% endif %}
					{% endfor %}

					{% if section.settings.megamenu_custom_link_enable %}
						{% assign customLinkHTML = "" %}

						{% for i in (1..8) %}
							{% assign custom_link_heading = 'megamenu_custom_link_' | append:i | append:"_text" %}
							{% assign custom_link_collection = 'megamenu_custom_link_' | append:i | append:"_collection" %}

							{% if section.settings[custom_link_heading] != "" %}
								{% if section.settings[custom_link_collection] != "" %}
									{% capture customLinkHTML %}
										{{ customLinkHTML }}

										<div class="section-wrapper-custom-links">
											<p>
												<a href="{{ section.settings[custom_link_collection] }}">{{ section.settings[custom_link_heading] }}
													<span class="separator"> » </span>
												</a>
											</p>
										</div>
									{% endcapture %}
								{% endif %}
							{% endif %}
						{% endfor %}
					{% endif %}

					{% if section.settings.enable_megamenu %}
						{% for block in section.blocks %}
                            {%- capture styles -%}
                              --collection-grid-font-size: {{- block.settings.collection_grid_font_size -}};
                              --collection-grid-font-style: {{- block.settings.collection_grid_font_style -}};
                  			  --collection-grid-font-color: {{- block.settings.collection_grid_font_color -}};
                            {%- endcapture -%}
                            {%- assign styles = styles | strip_newlines -%}
							{% if block.settings.megamenu_1_enable %}
								{% assign text = 'megamenu_' | append:i  | append:"_text" %}
								{% assign default_collection = 'megamenu_' | append:i  | append:"_default_collection" %}
								{% assign default_navcollection = 'megamenu_' | append:i  | append:"_linkcollection" %}
								{% assign enable_icon = 'megamenu_' | append:i  | append:"_icon_enable" %}
								{% assign totalEnable = 0 %}
								{% assign totalEnableValidate = 0 %}

								{% for j in (1..9) %}
									{% assign submenu_heading = 'megamenu_1' | append:"_submenu_" | append:j | append:"_heading" %}
									{% assign submenu_menu = 'megamenu_1' | append:"_submenu_" | append:j %}

									{% if block.settings[submenu_menu] != "" %}
										{% assign totalEnableValidate = totalEnableValidate | plus: 1 %}
									{% endif %}
								{% endfor %}

								{% assign megamenuwidthclass = 'widthfull' %}
								{% assign menuwrapperwidthclass = 'wraperwidthfull' %}
								{% assign menuprodlimit = 0 %}

								{% if totalEnableValidate == 1 %}
									{% assign menuprodlimit = 4 %}
								{% elsif totalEnableValidate == 2 %}
									{% assign menuprodlimit = 3 %}
								{% elsif totalEnableValidate == 3 %}
									{% assign menuprodlimit = 2 %}
								{% elsif totalEnableValidate == 5 %}
									{% assign menuprodlimit = 2 %}
								{% elsif totalEnableValidate == 6 %}
									{% assign menuprodlimit = 2 %}
								{% elsif totalEnableValidate == 7 %}
									{% assign menuprodlimit = 2 %}
								{% else %}
									{% assign menuprodlimit = 2 %}
								{% endif %}

								{% if block.settings.megamenu_1_collection %}
									<li aria-haspopup="true" id="li-main-for-{% increment counter %}"
										class="menu-dropdown-icon js-dropdown-menu-link" style="{{- styles -}}">
										<a class="menu-open"
										   href="{% if block.settings.megamenulink %}{{ block.settings.megamenulink }}{% else %}#{% endif %}">
											{{ block.settings.title }}

											<span class="fa fa-angle-down"
												  id="arrow-for-{% increment counter %}"></span>
										</a>

										<ul id="menu-for-{% increment counter %}" class="fulwidtht normal-sub menu-for">
											<div class="mega_Wrap clearfix">
												<div class="col-md-12 equal-line">
													<div class="row">
														<div class="menu-hei">
															{% for link in linklists[block.settings.megamenu_1_linkcollection].links %}
																{% if link.type == 'collection_link' %}
																	{% assign collection = link.object %}

																	<div class="col-md-2">
																		<div>
																			<div class="text-center">
																				<a href="{{ collection.url }}"
																				   class="collection-grid-item__link">

																					{% if collection.image %}
																						{% assign src = collection | img_url: '200x' %}
																					{% else %}
																						{% assign src = collection.products.first | img_url:  '200x' %}
																					{% endif %}

																					<img alt="{{ collection.title }}" src="{{ src }}" width="300px">

																					<div class="collection-grid-item__title-wrapper">
																						<h3 class="navcol">
																							{{ collection.title }}
																						</h3>
																					</div>
																				</a>
																			</div>
																		</div>
																	</div>
																{% endif %}
															{% endfor %}
														</div>
													</div>
												</div>
											</div>
										</ul>
									</li>
